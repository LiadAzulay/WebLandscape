
<% include ../partials/header %>

    <div class="container">
        <header class="jumbotron">
            <div class="container">

                <h1>This is the landscape page</h1>
                <p>view our landscapes from all over the world</p>
                <p>
                    <a calss="btn btn-primary btn-large" href="/landscapes/new">add new landscape</a>
                </p>
            </div>
        </header>
    <div class="row text-center" style="display:flex; flex-wrap: wrap;">
            <div class="col-lg-12">
                <h3>Our Most nicest landscapes</h3>
            </div>
            <% landscapes.forEach(function(landscape){%>
                <div class="col-md-3 col-sm-6">

                    
                    <div class="thumbnail">
                        <img src="<%= landscape.image%>">
                    </div>
                                        <div class="caption">
                        <h4><%=landscape.name%></h4>     
                    </div>
                    <p>
                        <a href="/landscapes/<%= landscape._id%>" class="btn btn-primary">More Info</a>
                    </p>
                </div>
                
            <% }); %>
        </div>
    </div>


<% include ../partials/footer %>